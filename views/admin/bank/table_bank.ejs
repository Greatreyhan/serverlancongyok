          <!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <a href="/admin/bank/add" class="btn btn-success">Add Bank</a>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>Name Bank</th>
                      <th>Nomor Rekening</th>
                      <th>Atas Nama</th>
                      <th>Logo</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(let i = 0; i < bank.length; i++){ %>
                      <tr>
                        <td><%= bank[i].nameBank %></td>
                        <td><%= bank[i].nomorRekening %></td>
                        <td><%= bank[i].name %></td>
                        <td><img src="http://localhost:3000/<%= bank[i].imageUrl %>" alt="imageUrl" width="70px" height="100%" /></td>
                        <td>
                          <form action="/admin/bank/delete/<%= bank[i].id %>?_method=DELETE" method="POST" >
                            <button type="submit" class="btn btn-danger btn-sm"><i class="fas fa-trash"></i></button>
                          </form>
                        </td>
                      </tr>
                    <% } %>
                  </tbody>

                </table>
              </div>
            </div>
          </div>