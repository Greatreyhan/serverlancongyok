          <!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <a href="/admin/item/activity/add/<%= item._id %>"" class="btn btn-success">Add Activity</a>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>Name</th>
                      <th>Type</th>
                      <th>Image</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(let i = 0; i < item.activityId.length; i++){ %>
                      <tr>
                        <td><%= item.activityId[i].name %></td>
                        <td><%= item.activityId[i].type %></td>
                        <td><img src="http://localhost:3000/<%= item.activityId[i].imageUrl %>" alt="imageUrl" width="70px" height="100%" /></td>
                        <td>
                          <form action="/admin/item/activity/delete/<%= item.activityId[i]._id %>?_method=DELETE" method="POST" >
                            <button type="submit" class="btn btn-danger btn-sm"><i class="fas fa-trash"></i></button>
                          </form>
                        </td>
                      </tr>
                    <% } %>
                  </tbody>

                </table>
              </div>
            </div>
          </div>